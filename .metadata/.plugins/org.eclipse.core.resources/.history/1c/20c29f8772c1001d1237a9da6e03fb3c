package infiniteLoop;

import java.util.*;

public class InfiniteLoop3 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        
        while (true) {
            int n = scanner.nextInt();
            switch(n) {
                case 1:
                    System.out.println("John");
                case 1:
                    System.out.println("John");
                case 1:
                    System.out.println("John");
                case 1:
                    System.out.println("John");
            }
        }
    }

}
