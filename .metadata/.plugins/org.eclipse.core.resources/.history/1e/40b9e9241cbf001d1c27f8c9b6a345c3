package ifInFor;

import java.util.*;

public class IfinFor1 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        
        for (int i = 0; i < n; i++) {
            if (i % 2 == 0 || i % 3 == 0) {
                System.out.print(1 );
            }
        }

    }

}
