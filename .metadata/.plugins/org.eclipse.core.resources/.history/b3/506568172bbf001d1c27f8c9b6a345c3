package sum;

import java.util.*;

public class Sum7 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int sumVal = 0;
        int cnt = 0;
        for (int i = 0; i < 10; i++) {
            int a = scanner.nextInt();
            if (a >= 0 && a <= 200) {
                sumVal += a;
                cnt++;
            }
        }
        double avg = (double) sumVal / cnt; 
        System.out.printf(sumVal + " " + avg);
    }

}
