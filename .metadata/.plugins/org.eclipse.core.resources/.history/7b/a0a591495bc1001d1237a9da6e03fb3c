package daySixth.Game;

public class Hero {
    // field variable, global variable, member variable, property
    String name;
    int hp;
    // field variable, global variable, member variable, property
    
    void attack() {}

    void run() {}

    void sit(int sec) {}

    void slip() {}

    void sleep() {
        this.hp = 100;
        System.out.println(this.name + "");
    }
}
