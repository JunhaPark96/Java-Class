package repeatNtime;

import java.util.*;

public class RepeatNtime3 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        int ans = 0;
        for (int i = 0; i < 20; i++) {
            ans = a / b;
            System.out.print(a / b);
            ans /= 10;

        }
    }

}
