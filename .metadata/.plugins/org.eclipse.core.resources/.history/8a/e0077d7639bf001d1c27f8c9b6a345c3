package breakEx;

import java.util.*;

public class Break5 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int cnt = 0;

        for (int i = 1; i < n; i++) {
            cnt++;
            n /= i;
            if (n < 1) {
                break;
            }
        }
        System.out.println(cnt);

    }

}
