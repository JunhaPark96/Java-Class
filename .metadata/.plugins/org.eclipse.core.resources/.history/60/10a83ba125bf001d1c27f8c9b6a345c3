package cnt;

import java.util.*;

public class Cnt5 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int cnt = 0;
        
        for (int i = 0; i < 5; i++) {
            int n = scanner.nextInt();
            if (n % 2 == 0) {
                cnt++;
            }
        }

    }

}
