package ifInFor;
/*
 * 
 */
import java.util.*;

public class IfinFor5 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        
        for (int i = 0; i < n; i++) {
            int a = scanner.nextInt();
            if (a % 2 != 0 && a % 3 == 0) {
                System.out.println(a);
            }
        }

    }

}
