package satisfiedOne;

import java.util.*;

public class SatisfiedOne2 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        boolean ans = false;
        
        for (int i = 1; i < n; i++) {
            if (n % i == 0) {
                ans = !ans;
            }
        }
        if (ans) {
            System.out.println("");
        } else {
            System.out.println("");
        }
    }

}
