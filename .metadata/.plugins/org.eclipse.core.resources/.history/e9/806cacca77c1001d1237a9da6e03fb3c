package doNotKnowLoop;

import java.util.*;

public class DoNotKnowLoop6 {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int cnt = 0;
        
        int n = scanner.nextInt();
        while (true) {
            if (n == 1) {
                break;
            }
            n /= 2;
        }

    }

}
