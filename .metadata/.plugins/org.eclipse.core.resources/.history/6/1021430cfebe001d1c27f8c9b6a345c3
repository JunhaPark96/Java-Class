package dayFifth.Main;

public class DayFifthQuiz3 {

    public static void main(String[] args) {
        // 전자시계 프로그램을 작성하시오
        int hour = 1;
        for (int j = 0; j < 60; j++) {
            switch (hour) {
                case 13:
                    break;
                default:
                    System.out.printf("%d:%02d ", hour, j);
            }
            // hour++;
            // if (hour > 12) {
            // break;
            // }
            System.out.println();
        }
    }

}
