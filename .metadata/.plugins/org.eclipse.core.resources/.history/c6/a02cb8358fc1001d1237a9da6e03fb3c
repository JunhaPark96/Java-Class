package star.rightTriangle;

import java.util.*;

public class Star3 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        
        for (int i = n; i > 0; i--) {
            for (int j = i; j > 0; j--) {
                System.out.print("* ");
                System.out.print("*");
            }
            System.out.println();
        }
    }

}
