package DayFourthMethod;

public class DayFourthSelf {
    
    public static void sum(String s, String...str) {
        for(String a:str)
            System.out.print(a+s);
        }

    
    public static void main(String[] args) {
        // TODO Auto-generated method stub
        String v;
        v.sum("-","a","b","c");

    }

}
