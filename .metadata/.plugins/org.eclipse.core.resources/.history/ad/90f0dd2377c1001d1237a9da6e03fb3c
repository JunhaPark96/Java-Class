package doNotKnowLoop;

public class DoNotKnowLoop6 {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        int a = 2;
        int b = 2;
        while(b > 0)
        {  
            a++;    
            b--;     
        }
        do{    
            a--;    
            b++;
        } while(a > 0);
        System.out.println(b);

    }

}
