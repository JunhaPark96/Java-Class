package dayFifth.Main;

public class DayFifthQuiz2 {

    public static void main(String[] args) {
        for (int i = 1; i < 10; i++) {
            // System.out.print(i + " * ");
            for (int j = 2; j < 10; j++) {
                System.out.printf(j + " * " + i + " = " + j * i + "\t");
            }
            System.out.println();
        }
    }

}
